/**index.wxss**/
swiper,.slider,.slider image{
  width: 100%;
  height: 390rpx;
}
.index-nav{
  display: flex;
  flex-wrap: wrap;
}
.nav-item{
  width: 33.3%;
  height: 200rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  border-right: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
}
.nav-item:nth-child(3n){
  border-right: 0;
}
.nav-item image{
  width: 102rpx;
  height: 102rpx;
}
.nav-item text{
  margin-top: 5px;
}
.divider{
  height: 30rpx;
  background-color: #f1f1f1;
}